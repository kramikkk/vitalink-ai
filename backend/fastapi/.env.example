# Example .env file for FastAPI backend
# Generate a secure SECRET_KEY by running: openssl rand -hex 32
SECRET_KEY=your_secret_key_here

# JWT Configuration
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60

# Database Configuration
# sqlite for local development, replace with your production database URL as needed
DATABASE_URL=sqlite:///./vitalink.db

# Comma-separated list of allowed origins for CORS
ALLOWED_ORIGINS=http://localhost:3000,http://192.168.1.12:3000
